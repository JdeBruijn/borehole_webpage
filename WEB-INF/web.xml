<?xml version="1.0" encoding="UTF-8"?>
<web-app>
        <display-name>Borehole</display-name>
        <welcome-file-list>
                <welcome-file>/index.jsp</welcome-file>
        </welcome-file-list>

        <servlet>
                <servlet-name>MainServlet</servlet-name>
                <servlet-class>borehole.MainServlet</servlet-class>
        </servlet>
        <servlet-mapping>
                <servlet-name>MainServlet</servlet-name>
                <url-pattern>/main-servlet/*</url-pattern>
        </servlet-mapping>

</web-app>

